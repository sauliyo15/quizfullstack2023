    "express-flash": "^0.0.2",
    "express-partials": "^0.3.0",
    "express-session": "^1.17.3",
    "http-errors": "~1.6.3",
    "method-override": "^3.0.0",
    "morgan": "~1.9.1",
    "sequelize": "^5.21.5",
    "sequelize-cli": "^5.5.1",
    "serve-favicon": "^2.5.0",
    "sqlite3": "^5.1.6",
    "supervisor": "^0.12.0"
  }
